<launch>
  <arg name = "startX" value="10" /> <!--  X coordinate of the starting point -->
  <arg name = "startY" value="10" /> <!--  Y coordinate of the starting point -->
  <arg name = "endX" value="480" /> <!--  X coordinate of the goal -->
  <arg name = "endY" value="480" /> <!--  Y coordinate of the goal -->
  <arg name = "stepSize" value="10.0" /> <!--step size of RRT -->
  <arg name = "algoSpeed" value="100" /> <!--step size of RRT -->
  <arg name = "maxRuns" value="4" /> <!--step size of RRT -->
  <arg name = "neighbourFactor" value="3.0"/> <!--neighbour Factor for search-->
  <arg name = "costToGoFactor" value="3.0"/> <!--neighbour Factor for search-->
  <arg name = "maxIter" value="20000" /><!--max iterations of the RRT algo-->
  <arg name="map_file" value="/home/raakesh/Documents/cppprojects/catkin_ws/src/anytimerrt/maps/map.yaml">
  </arg> 
  <node pkg="map_server" type="map_server" name="map_server" args="$(arg map_file)"> 
  </node>
  <node pkg="anytimerrt" type="anytimerrt_node" name="planner" 
                  args="_startX:=$(arg startX) _startY:=$(arg startY) _endX:=$(arg endX) _endY:=$(arg endY) _stepSize:=$(arg stepSize) 
                        _algoSpeed:=$(arg algoSpeed) _maxRuns:=$(arg maxRuns) _neighbourFactor:=$(arg neighbourFactor) _costToGoFactor:=$(arg costToGoFactor) _maxIter:=$(arg maxIter)" 
                          output="screen"> 
  </node> 
</launch>